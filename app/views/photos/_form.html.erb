<%= simple_form_for(@photo) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :flickr_id %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :date_taken %>
    <%= f.input :thumb %>
    <%= f.input :small %>
    <%= f.input :medium %>
    <%= f.input :large %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
